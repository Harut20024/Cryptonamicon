{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      ticker: \"\",\n      sell: null,\n      tickers: [],\n      graph: [],\n      updateInterval: null,\n      page: 1,\n      coinSymbols: [],\n      maxGraphElements: 1,\n      filter: \"\"\n    };\n  },\n  created() {\n    this.fetchCoins();\n    const tickersData = localStorage.getItem(\"cryptonimicon-list\");\n    if (tickersData) {\n      this.tickers = JSON.parse(tickersData);\n      this.updateTickers();\n    }\n  },\n  computed: {\n    startIndex() {\n      return (this.page - 1) * 6;\n    },\n    endIndex() {\n      return this.page * 6;\n    },\n    filteredTickers() {\n      return this.tickers.filter(ticker => ticker.name.includes(this.filter));\n    },\n    paginatedTickers() {\n      return this.filteredTickers.slice(this.startIndex, this.endIndex);\n    },\n    normalizedGraph() {\n      const maxValue = Math.max(...this.graph);\n      const minValue = Math.min(...this.graph);\n      if (maxValue === minValue) return this.graph.map(() => 50);\n      return this.graph.map(price => 5 + (price - minValue) * 95 / (maxValue - minValue));\n    }\n  },\n  watch: {\n    graph(newGraph) {\n      if (newGraph.length > 0) {\n        this.calculateMaxGraphElements();\n      }\n    },\n    paginatedTickers() {\n      if (this.paginatedTickers.length === 0 && this.page > 1) {\n        this.page -= 1;\n      }\n    }\n  },\n  mounted() {\n    this.updateTickers();\n    if (this.sell) {\n      this.fetchGraphData(this.sell);\n    }\n    // Initialize the update interval\n    this.updateInterval = setInterval(this.updateTickers, 2000);\n  },\n  beforeUnmount() {\n    window.removeEventListener(\"resize\", this.calculateMaxGraphElements);\n  },\n  methods: {\n    calculateMaxGraphElements() {\n      if (!this.$refs.graph) {\n        return;\n      }\n      this.maxGraphElements = this.$refs.graph.clientWidth / 38;\n    },\n    async add() {\n      if (this.tickers.some(t => t.name === this.ticker)) {\n        alert(\"Ticker already added\");\n        return;\n      }\n      const newItem = {\n        name: this.ticker.toUpperCase(),\n        value: \"-\"\n      };\n      this.tickers.push(newItem);\n      localStorage.setItem(\"cryptonimicon-list\", JSON.stringify(this.tickers));\n      this.ticker = \"\";\n      this.filter = \"\";\n      if (!this.updateInterval) {\n        this.updateInterval = setInterval(this.updateTickers, 2000);\n      }\n    },\n    async fetchCoins() {\n      try {\n        const response = await fetch(\"https://min-api.cryptocompare.com/data/all/coinlist?summary=true\");\n        const data = await response.json();\n        if (data.Response === \"Success\" && data.Data) {\n          this.coinSymbols = Object.values(data.Data);\n        }\n      } catch (error) {\n        console.error(\"Error fetching coins:\", error);\n      }\n    },\n    filteredCoins() {\n      if (!this.ticker) return;\n      return this.coinSymbols.filter(coin => coin.Symbol.toUpperCase().includes(this.ticker.toUpperCase())).slice(0, 6);\n    },\n    async updateTickers() {\n      if (this.tickers.length === 0) {\n        clearInterval(this.updateInterval);\n        this.updateInterval = null;\n        return;\n      }\n      for (const ticker of this.tickers) {\n        try {\n          const response = await fetch(`https://min-api.cryptocompare.com/data/price?fsym=${ticker.name}&tsyms=USD&api_key=your_api_key`);\n          const data = await response.json();\n          ticker.value = data.USD;\n        } catch (error) {\n          console.error(\"Fetch error:\", error);\n        }\n      }\n    },\n    select(ticker) {\n      this.sell = ticker;\n      this.fetchGraphData(ticker);\n    },\n    async fetchGraphData(ticker) {\n      try {\n        const response = await fetch(`https://min-api.cryptocompare.com/data/price?fsym=${ticker.name}&tsyms=USD&api_key=your_api_key`);\n        const data = await response.json();\n        this.graph = [data.USD]; // Starting new graph data array\n        // Adjust the maxGraphElements based on the new graph\n        this.calculateMaxGraphElements();\n      } catch (error) {\n        console.error(\"Error fetching graph data:\", error);\n      }\n    },\n    dell(deltick) {\n      this.tickers = this.tickers.filter(t => t.name !== deltick.name);\n      if (this.sell && this.sell.name === deltick.name) {\n        this.sell = null;\n        this.graph = [];\n      }\n      localStorage.setItem(\"cryptonimicon-list\", JSON.stringify(this.tickers));\n      if (this.tickers.length === 0 && this.updateInterval) {\n        clearInterval(this.updateInterval);\n        this.updateInterval = null;\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","ticker","sell","tickers","graph","updateInterval","page","coinSymbols","maxGraphElements","filter","created","fetchCoins","tickersData","localStorage","getItem","JSON","parse","updateTickers","computed","startIndex","endIndex","filteredTickers","includes","paginatedTickers","slice","normalizedGraph","maxValue","Math","max","minValue","min","map","price","watch","newGraph","length","calculateMaxGraphElements","mounted","fetchGraphData","setInterval","beforeUnmount","window","removeEventListener","methods","$refs","clientWidth","add","some","t","alert","newItem","toUpperCase","value","push","setItem","stringify","response","fetch","json","Response","Data","Object","values","error","console","filteredCoins","coin","Symbol","clearInterval","USD","select","dell","deltick"],"sources":["/home/harut/Desktop/cryptonomicon/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto flex flex-col items-center bg-gray-100 p-4\">\n    <div class=\"container\">\n      <section>\n        <div class=\"flex\">\n          <div class=\"max-w-xs\">\n            <label for=\"wallet\" class=\"block text-sm font-medium text-gray-700\"\n              >Тикер</label\n            >\