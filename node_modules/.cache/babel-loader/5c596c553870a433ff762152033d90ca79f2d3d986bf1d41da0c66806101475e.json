{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      ticker: \"\",\n      sell: null,\n      tickers: [],\n      graph: []\n    };\n  },\n  methods: {\n    add() {\n      const newItem = {\n        name: this.ticker,\n        value: \"-\"\n      };\n      this.tickers.push(newItem);\n      setInterval(async () => {\n        const f = await fetch(`https://min-api.cryptocompare.com/data/price?fsym=${newItem.name}&tsyms=USD&api_key=9384a78ce72773fbcea39ffc4a0f7e5c06488b4d220f2bf1677a35fc07d5f879`);\n        const data = await f.json();\n        this.tickers.find(t => t.name === newItem.name).value = data.USD;\n        if (this.sell && this.sell.name === newItem.name) {\n          this.graph.push(data.USD);\n        }\n      }, 3000);\n      this.ticker = \"\";\n    },\n    dell(deltick) {\n      this.tickers = this.tickers.filter(t => t != deltick);\n    }\n  }\n};","map":{"version":3,"names":["name","data","ticker","sell","tickers","graph","methods","add","newItem","value","push","setInterval","f","fetch","json","find","t","USD","dell","deltick","filter"],"sources":["/home/harut/Desktop/cryptonomicon/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto flex flex-col items-center bg-gray-100 p-4\">\n    <div class=\"container\">\n      <section>\n        <div class=\"flex\">\n          <div class=\"max-w-xs\">\n            <label for=\"wallet\" class=\"block text-sm font-medium text-gray-700\">Тикер</label>\n            <div class=\"mt-1 relative rounded-md shadow-md\">\n              <input\n                v-model=\"ticker\"\n                @keydown.enter=\"add()\"\n                type=\"text\"\n                name=\"wallet\"\n                id=\"wallet\"\n                class=\"block w-full pr-10 border-gray-300 text-gray-900 focus:outline-none focus:ring-gray-500 focus:border-gray-500 sm:text-sm rounded-md\"\n                placeholder=\"Например DOGE\"\n              />\n            </div>\n            <div v-if=\"duplicateTicker\" class=\"text-sm text-red-600\">Такой тикер уже добавлен</div>\n          </div>\n        </div>\n        <button\n          @click=\"add()\"\n          type=\"button\"\n          class=\"my-4 inline-flex items-center py-2 px-4 border border-transparent shadow-sm text-sm leading-4 font-medium rounded-full text-white bg-gray-600 hover:bg-gray-700 transition-colors duration-300 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-gray-500\"\n        >\n          Добавить\n        </button>\n      </section>\n\n      <template v-if=\"tickers.length\">\n        <hr class=\"w-full border-t border-gray-600 my-4\" />\n        <dl class=\"mt-5 grid grid-cols-1 gap-5 sm:grid-cols-3\">\n          <div\n            v-for=\"tick in tickers\"\n            :key=\"tick.name\"\n            @click=\"selectTicker(tick)\"\n            :class=\"{'border-4': sell === tick}\"\n            class=\"bg-white overflow-hidden shadow rounded-lg border-purple-800 border-solid cursor-pointer\"\n          >\n            <div class=\"px-4 py-5 sm:p-6 text-center\">\n              <dt class=\"text-sm font-medium text-gray-500 truncate\">{{ tick.name }}</dt>\n              <dd class=\"mt-1 text-3xl font-semibold text-gray-900\">{{ tick.value }}</dd>\n            </div>\n            <div class=\"w-full border-t border-gray-200\"></div>\n            <button\n              @click.stop=\"dell(tick)\"\n              class=\"flex items-center justify-center font-medium w-full bg-gray-100 px-4 py-4 sm:px-6 text-md text-gray-500 hover:text-gray-600 hover:bg-gray-200 hover:opacity-20 transition-all focus:outline-none\"\n            >\n              Удалить\n            </button>\n          </div>\n        </dl>\n        <hr class=\"w-full border-t border-gray-600 my-4\" />\n      </template>\n\n      <section v-if=\"sell\" class=\"relative\">\n        <h3 class=\"text-lg leading-6 font-medium text-gray-900 my-8\">{{ sell.name }} - USD</h3>\n        <div class=\"flex\">\n          <div\n            v-for=\"(bar, ind) in graph\"\n            :key=\"ind\"\n            :style=\"{ height: `${bar}%` }\"\n            class=\"bg-purple-800 border w-10 mx-1\"\n          ></div>\n        </div>\n        <button @click=\"sell = null\" type=\"button\" class=\"absolute top-0 right-0\">\n          <!-- Close button SVG here -->\n        </button>\n      </section>\n    </div>\n  </div>\n</template>\n\n\n<script>\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      ticker: \"\",\n      sell: null,\n      tickers: [],\n      graph: [],\n    };\n  },\n\n  methods: {\n    add() {\n      const newItem = {\n        name: this.ticker,\n        value: \"-\",\n      };\n\n      this.tickers.push(newItem);\n      setInterval(async () => {\n        const f = await fetch(\n          `https://min-api.cryptocompare.com/data/price?fsym=${newItem.name}&tsyms=USD&api_key=9384a78ce72773fbcea39ffc4a0f7e5c06488b4d220f2bf1677a35fc07d5f879`\n        );\n        const data = await f.json();\n        this.tickers.find((t) => t.name === newItem.name).value = data.USD;\n\n        if (this.sell && this.sell.name === newItem.name) {\n          this.graph.push(data.USD);\n        }\n      }, 3000);\n      this.ticker = \"\";\n    },\n    dell(deltick) {\n      this.tickers = this.tickers.filter((t) => t != deltick);\n    },\n  },\n};\n</script>\n\n<style src=\"./app.css\"></style>\n"],"mappings":";AA4EA,eAAe;EACbA,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,IAAI,EAAE,IAAI;MACVC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,GAAGA,CAAA,EAAG;MACJ,MAAMC,OAAM,GAAI;QACdR,IAAI,EAAE,IAAI,CAACE,MAAM;QACjBO,KAAK,EAAE;MACT,CAAC;MAED,IAAI,CAACL,OAAO,CAACM,IAAI,CAACF,OAAO,CAAC;MAC1BG,WAAW,CAAC,YAAY;QACtB,MAAMC,CAAA,GAAI,MAAMC,KAAK,CAClB,qDAAoDL,OAAO,CAACR,IAAK,qFACpE,CAAC;QACD,MAAMC,IAAG,GAAI,MAAMW,CAAC,CAACE,IAAI,CAAC,CAAC;QAC3B,IAAI,CAACV,OAAO,CAACW,IAAI,CAAEC,CA