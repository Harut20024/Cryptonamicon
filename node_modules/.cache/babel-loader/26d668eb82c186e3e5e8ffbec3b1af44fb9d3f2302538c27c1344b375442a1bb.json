 fetchCoins() {\n      try {\n        const response = await fetch(\n          \"https://min-api.cryptocompare.com/data/all/coinlist?summary=true\"\n        );\n        const data = await response.json();\n        if (data.Response === \"Success\" && data.Data) {\n          this.coinSymbols = Object.values(data.Data);\n        }\n      } catch (error) {\n        console.error(\"Error fetching coins:\", error);\n      }\n    },\n\n    filteredCoins() {\n      if (!this.ticker) return;\n      return this.coinSymbols\n        .filter((coin) =>\n          coin.Symbol.toUpperCase().includes(this.ticker.toUpperCase())\n        )\n        .slice(0, 6);\n    },\n\n    async updateTickers() {\n      if (this.tickers.length === 0) {\n        clearInterval(this.updateInterval);\n        this.updateInterval = null;\n        return;\n      }\n\n      for (const ticker of this.tickers) {\n        try {\n          const response = await fetch(\n            `https://min-api.cryptocompare.com/data/price?fsym=${ticker.name}&tsyms=USD&api_key=9384a78ce72773fbcea39ffc4a0f7e5c06488b4d220f2bf1677a35fc07d5f879`\n          );\n          const data = await response.json();\n          console.log(data);\n          ticker.value = data.USD;\n          if (this.sell && this.sell.name === ticker.name) {\n            this.graph.push(data.USD);\n          }\n        } catch (error) {\n          console.error(\"Fetch error:\", error);\n        }\n      }\n    },\n\n    select(ticker) {\n      this.sell = ticker;\n      this.graph = [];\n    },\n\n    dell(deltick) {\n      this.tickers = this.tickers.filter((t) => t.name !== deltick.name);\n\n      if (this.sell && this.sell.name === deltick.name) {\n        this.sell = null;\n        this.graph = [];\n      }\n\n      localStorage.setItem(\"cryptonimicon-list\", JSON.stringify(this.tickers));\n\n      if (this.tickers.length === 0 && this.updateInterval) {\n        clearInterval(this.updateInterval);\n        this.updateInterval = null;\n      }\n